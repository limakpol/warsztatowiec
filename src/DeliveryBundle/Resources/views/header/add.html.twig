{% extends ':base:application.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
    'assets/css/application/delivery/add.css'
    filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}
{% block main %}
    <h1 class="h-enum">1. Wybierz dostawcę spośród klientów lub dodaj nowego</h1>
    <div id="searchable-customer" class="searchable">
        <input type="text" placeholder="Wyszukaj dostawcę">
        <div>
             {% include 'DeliveryBundle:header:customer_searchable_content.html.twig' %}
        </div>
    </div>
    <h1 class="h-enum h-selected-customer">2. Przyjęcie wewnętrzne (brak dostawcy)</h1>
    <h1 class="h-enum">3. Możesz wpisać ręcznie całkowite wartości przyjęcia lub pozostawić je do automatycznego wyliczenia</h1>
    <h1 class="h-enum">4. Pozostałe dane</h1>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {% javascripts
    'assets/js/application/delivery/add.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}