{% extends ':base:application.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
        'assets/css/application/order/show.css'
        'assets/css/application/order/status.css'
    filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}
{% block main %}
    <div class="show-box">
        <h1>
            <label>Dane podstawowe</label>
        </h1>
        <section>
            <table class="info-table">
                <tr>
                    <td>Statusy</td>
                    <td class="statuses" data-order-id="{{ orderHeaderId }}">
                        {% for status in orderHeader.statuses %}
                            <button style="border-color: {{ status.hexColor }}">{{ status.name }}</button>
                        {% endfor %}
                    </td>
                </tr>
                <tr>
                    <td>Stanowisko naprawcze</td>
                    <td>
                        <div class="main-form">
                            <select id="select-workstation">
                                <option value=""></option>
                                {% for workstation in workstations %}
                                    <option value="{{ workstation.id }}"
                                    {% if workstation.id == orderHeader.workstationId %}
                                    selected="selected"
                                    {% endif %}
                                    >{{ workstation.name }}</option>
                                {% endfor %}
                            </select>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>Priorytet</td>
                    <td>
                        <input type="checkbox" id="checkbox-priority"
                        {% if orderHeader.priority == true %}
                        checked="checked"
                        {% endif %}
                        >
                    </td>
                </tr>
                <tr>
                    <td>Uwagi</td>
                    <td>{{ orderHeader.remarks }}</td>
                </tr>
                <tr>
                    <td>Zakończone</td>
                    <td>
                        <button id="order-completed" class="date-or-no">
                            {% if orderHeader.completedAt is null %}
                            NIE
                            {% else %}
                                {{ orderHeader.completedAt|date }}
                            {% endif %}
                        </button>
                    </td>
                </tr>
                <tr>
                    <td>Zapłacone</td>
                    <td>
                        <button id="order-paid" class="date-or-no">
                            {% if orderHeader.paidAt is null %}
                                NIE
                            {% else %}
                                {{ orderHeader.paidAt|date }}
                            {% endif %}
                        </button>
                    </td>
                </tr>
                <tr>
                    <td>Kwota wpłacona przez klienta</td>
                    <td>
                        <div class="main-form form-set">
                            <input type="hidden" id="paid-amount" value="{{ orderHeader.amountPaid }}">
                            <input type="text" maxlength="10" size="6" class="input-disabled" id="input-paid" value="{{ orderHeader.amountPaid }}">zł
                            <div id="paid-buttons">
                                <button class="btn-save btn-square" data-add-i="floppy-o"><i class="fa fa-floppy-o" aria-hidden="true"></i>
                                </button>
                            </div>
                            <div id="paid-errors"></div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>Wprowadzono</td>
                    <td>{{ orderHeader.createdAt|date }} przez <a href="{{ path('workflow_workman_show', {userId: orderHeader.createdBy.id}) }}">
                            {{ orderHeader.createdBy.forename }} {{ orderHeader.createdBy.surname }}
                        </a>
                    </td>
                </tr>
                <tr>
                    <td>Zaktualizowano</td>
                    <td>
                        {{ orderHeader.updatedAt|date }} przez <a href="{{ path('workflow_workman_show', {userId: orderHeader.updatedBy.id}) }}">
                            {{ orderHeader.updatedBy.forename }} {{ orderHeader.updatedBy.surname }}
                        </a>
                    </td>
                </tr>
                <tr class="footer">
                    <td colspan="2">
                        <button class="btn-remove" id="order-remove">usuń<i class="fa fa-trash-o" aria-hidden="true"></i></button>
                    </td>
                </tr>
            </table>
        </section>
    </div>
    <div class="show-box">
        <h1>
            <label>Klient</label>
        </h1>
        <section>
            <table class="info-table">
                <tr>
                    <td>Imię</td>
                    <td>{{ orderHeader.customer.forename }}</td>
                </tr>
                <tr>
                    <td>Nazwisko</td>
                    <td>{{ orderHeader.customer.surname }}</td>
                </tr>
                <tr>
                    <td>Nazwa firmy</td>
                    <td>
                        {{ orderHeader.customer.companyName }}
                    </td>
                </tr>
                <tr>
                    <td>Telefon</td>
                    <td>
                        {% if orderHeader.customer.mobilePhone1 is null%}
                            {{ orderHeader.customer.mobilePhone2|phone }}
                        {% else %}
                            {{ orderHeader.customer.mobilePhone1|phone }}
                        {% endif %}
                    </td>
                </tr>
                <tr>
                    <td>E-mail</td>
                    <td>{{ orderHeader.customer.email }}</td>
                </tr>
                <tr class="footer">
                    <td colspan="2">
                        <a class="btn-info" href="{{ path('customer_show', {customerId: orderHeader.customerId}) }}">szczegóły
                            <i class="fa fa-info-circle" aria-hidden="true"></i>
                        </a>
                        <button class="btn-edit">zmień
                            <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                        </button>
                    </td>
                </tr>
            </table>
        </section>
    </div>
    <div class="show-box">
        <h1>
            <label>Pojazd</label>
        </h1>
        <section>
            <table class="info-table">
                <tr>
                    <td>Marka</td>
                    <td>
                        {{ orderHeader.vehicle.carModel.brand.name }}
                    </td>
                </tr>
                <tr>
                    <td>Model</td>
                    <td>{{ orderHeader.vehicle.carModel.name }}</td>
                </tr>
                <tr>
                    <td>Wersja</td>
                    <td>{{ orderHeader.vehicle.version }}</td>
                </tr>
                <tr>
                    <td>Typ silnika</td>
                    <td>{{ orderHeader.vehicle.engineType }}</td>
                </tr>
                <tr>
                    <td>Pojemność</td>
                    <td>
                        {% if orderHeader.vehicle.engineDisplacementL is not null %}
                            {{ orderHeader.vehicle.engineDisplacementL }}l
                        {% endif %}
                    </td>
                </tr>
                <tr class="footer">
                    <td colspan="2">
                        <a class="btn-info" href="{{ path('vehicle_show', {vehicleId: orderHeader.vehicleId}) }}">szczegóły
                            <i class="fa fa-info-circle" aria-hidden="true"></i>
                        </a>
                        <button class="btn-edit">zmień
                            <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                        </button>
                    </td>
                </tr>
            </table>
        </section>
    </div>
    <div class="show-box">
        <h1>
            <label>Objawy zgłaszane przez klienta</label>
        </h1>
        <section>
            <table class="show-inputable" data-add-path="{{ path('order_symptom_add') }}" data-remove-path="{{ path('order_symptom_remove') }}">
                {% for orderSymptom in orderHeader.symptoms %}
                    {% if orderSymptom.removedAt is null and orderSymptom.deletedAt is null %}
                        <tr class="present">
                            <td>{{ orderSymptom.name }}</td>
                            <td>
                                <button class="btn-square btn-remove" data-id="{{ orderSymptom.id }}">
                                    <i class="fa fa-trash-o" aria-hidden="true"></i>
                                </button>
                            </td>
                        </tr>
                    {% endif %}
                {% endfor %}
                <tr class="add">
                    <td>
                        <input type="text">
                        <div class="select">
                            {% for symptom in symptoms %}
                                <div>{{ symptom.name }}</div>
                            {% endfor %}
                        </div>
                    </td>
                    <td>
                        <button class="btn-square btn-add">
                            <i class="fa fa-plus" aria-hidden="true"></i>
                        </button>
                    </td>
                </tr>
            </table>
        </section>
    </div>
    <div class="show-box">
        <h1>
            <label>Usterki stwierdzone przez mechaników</label>
        </h1>
        <section>
            <table class="show-inputable" data-add-path="{{ path('order_fault_add') }}" data-remove-path="{{ path('order_fault_remove') }}">
                {% for orderFault in orderHeader.faults %}
                    {% if orderFault.removedAt is null and orderFault.deletedAt is null %}
                        <tr class="present">
                            <td>{{ orderFault.name }}</td>
                            <td>
                                <button class="btn-square btn-remove" data-id="{{ orderFault.id }}">
                                    <i class="fa fa-trash-o" aria-hidden="true"></i>
                                </button>
                            </td>
                        </tr>
                    {% endif %}
                {% endfor %}
                <tr class="add">
                    <td>
                        <input type="text">
                        <div class="select">
                            {% for fault in faults %}
                                <div>{{ fault.name }}</div>
                            {% endfor %}
                        </div>
                    </td>
                    <td>
                        <button class="btn-square btn-add">
                            <i class="fa fa-plus" aria-hidden="true"></i>
                        </button>
                    </td>
                </tr>
            </table>
        </section>
    </div>
    <div class="show-box">
        <h1>
            <label>Pozycje zlecenia</label>
            <a class="btn-add" href="{{ path('order_indexx_add', {orderHeaderId: orderHeaderId}) }}">dodaj towar<i class="fa fa-plus" aria-hidden="true"></i></a>
            <a class="btn-add" href="{{ path('order_service_add', {orderHeaderId: orderHeaderId}) }}">dodaj usługę<i class="fa fa-plus" aria-hidden="true"></i></a>
        </h1>
        <section>
            <table class="details-table">
                {% if orderHeader.orderIndexxes.count > 0 %}
                    <tr>
                        <th>L.p.</th>
                        <th>Nazwa towaru</th>
                        <th>Indeks</th>
                        <th>Producent</th>
                        <th>Cena jedn. netto</th>
                        <th>Ilość</th>
                        <th>J.m.</th>
                        <th>Netto</th>
                        <th>VAT</th>
                        <th>Brutto</th>
                    </tr>
                    {% for key, orderIndexx in orderHeader.orderIndexxes %}
                        <tr class="header">
                            <td class="td-center num">{{ key+1 }}.</td>
                            <td class="td-left">{{ orderIndexx.indexx.good.name }}</td>
                            <td class="td-left">{{ orderIndexx.indexx.name }}</td>
                            <td class="td-left">{{ orderIndexx.indexx.producer.name }}</td>
                            <td class="td-center">{{ orderIndexx.unitPriceNet|money }}</td>
                            <td class="td-center">{{ orderIndexx.quantity|money }}</td>
                            <td class="td-center">{{ orderIndexx.indexx.good.measure.shortcut }}</td>
                            <td class="td-center">{{ orderIndexx.totalNet|money }}</td>
                            <td class="td-center">{{ orderIndexx.vat|money }}</td>
                            <td class="td-center">{{ orderIndexx.totalDue|money }}</td>
                        </tr>
                        <tr class="content">
                            <td colspan="10">
                                <div class="content">
                                    <table class="info-table">
                                        <tr>
                                            <td>Towar</td>
                                            <td>
                                                <a href="{{ path('warehouse_good_show', {'goodId': orderIndexx.indexx.good.id}) }}">{{ orderIndexx.indexx.good.name }}</a>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Indeks</td>
                                            <td>{{ orderIndexx.indexx.name }}</td>
                                        </tr>
                                        <tr>
                                            <td>Producent</td>
                                            <td>{{ orderIndexx.indexx.producer.name }}</td>
                                        </tr>
                                        <tr>
                                            <td>Cena jednostkowa netto</td>
                                            <td>{{ orderIndexx.unitPriceNet|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Ilość</td>
                                            <td>{{ orderIndexx.quantity|money }} {{ orderIndexx.indexx.good.measure.shortcut }}</td>
                                        </tr>
                                        <tr>
                                            <td>Wartość netto przed rabatem</td>
                                            <td>{{ orderIndexx.totalNetBeforeDiscount|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Rabat</td>
                                            <td>{{ orderIndexx.discountPc }}%</td>
                                        </tr>
                                        <tr>
                                            <td>Kwota rabatu</td>
                                            <td>{{ orderIndexx.discount|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Wartość netto</td>
                                            <td>{{ orderIndexx.totalNet|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>VAT</td>
                                            <td>{{ orderIndexx.vatPc }}%</td>
                                        </tr>
                                        <tr>
                                            <td>Kwota VAT</td>
                                            <td>{{ orderIndexx.vat|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Należność całkowita</td>
                                            <td>{{ orderIndexx.totalDue|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Uwagi</td>
                                            <td>{{ orderIndexx.remarks }}</td>
                                        </tr>
                                        <tr>
                                            <td>Wprowadzono</td>
                                            <td>
                                                {{ orderIndexx.createdAt|date }} przez
                                                <a href="{{ path('workflow_workman_show', {userId: orderIndexx.createdBy.id}) }}">
                                                    {{ orderIndexx.createdBy.forename }} {{ orderIndexx.createdBy.surname }}</a>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Zaktualizowano</td>
                                            <td>
                                                {{ orderIndexx.updatedAt|date }} przez
                                                <a href="{{ path('workflow_workman_show', {userId: orderIndexx.updatedBy.id}) }}">
                                                    {{ orderIndexx.updatedBy.forename }} {{ orderIndexx.updatedBy.surname }}</a>
                                            </td>
                                        </tr>
                                        <tr class="footer">
                                            <td colspan="2">
                                                <a class="btn-edit" target="_blank">edytuj
                                                    <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                                </a>
                                                <button class="btn-remove">usuń
                                                    <i class="fa fa-trash-o" aria-hidden="true"></i>
                                                </button>
                                                <a class="btn-add" href="{{ path('order_action_add', {orderHeaderId: orderHeaderId, orderIndexxId: orderIndexx.id}) }}">dodaj czynność<i class="fa fa-plus" aria-hidden="true"></i>
                                                </a>
                                            </td>
                                        </tr>
                                    </table>
                                    <table class="details-table">
                                        <tr class="sub-header">
                                            <th>L.p.</th>
                                            <th>Czynność</th>
                                            <th>Cena jedn. netto</th>
                                            <th>Ilość</th>
                                            <th>J.m.</th>
                                            <th>Netto</th>
                                            <th>VAT</th>
                                            <th>Brutto</th>
                                        </tr>
                                        {% for key, orderAction in orderIndexx.orderActions %}
                                            <tr class="header">
                                                <td class="td-center num">{{ key+1 }}.</td>
                                                <td class="td-left">{{ orderAction.action.name }}</td>
                                                <td class="td-center">{{ orderAction.unitPriceNet|money }}</td>
                                                <td class="td-center">{{ orderAction.quantity|money }}</td>
                                                <td class="td-center">
                                                    {% if orderAction.measure is not null %}
                                                        {{ orderAction.measure.shortcut }}
                                                    {% endif %}
                                                </td>
                                                <td class="td-center">{{ orderAction.totalNet|money }}</td>
                                                <td class="td-center">{{ orderAction.vat|money }}</td>
                                                <td class="td-center">{{ orderAction.totalDue|money }}</td>
                                            </tr>
                                            <tr class="content">
                                                <td colspan="10">
                                                    <div class="content">
                                                        <table class="info-table">
                                                            <tr>
                                                                <td>Nazwa usługi</td>
                                                                <td>
                                                                    <a href="{{ path('action_index') }}">{{ orderAction.action.name }}</a>
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>Cena jednostkowa netto</td>
                                                                <td>{{ orderAction.unitPriceNet|money }} zł</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Ilość</td>
                                                                <td>{{ orderAction.quantity|money }}
                                                                    {% if orderAction.measure is not null %}
                                                                        {{ orderAction.measure.shortcut }}
                                                                    {% endif %}
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>Wartość netto przed rabatem</td>
                                                                <td>{{ orderAction.totalNetBeforeDiscount|money }} zł</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Rabat</td>
                                                                <td>{{ orderAction.discountPc }}%</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Kwota rabatu</td>
                                                                <td>{{ orderAction.discount|money }} zł</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Wartość netto</td>
                                                                <td>{{ orderAction.totalNet|money }} zł</td>
                                                            </tr>
                                                            <tr>
                                                                <td>VAT</td>
                                                                <td>{{ orderAction.vatPc }}%</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Kwota VAT</td>
                                                                <td>{{ orderAction.vat|money }} zł</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Należność całkowita</td>
                                                                <td>{{ orderAction.totalDue|money }} zł</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Uwagi</td>
                                                                <td>{{ orderAction.remarks }}</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Wprowadzono</td>
                                                                <td>
                                                                    {{ orderAction.createdAt|date }} przez
                                                                    <a href="{{ path('workflow_workman_show', {userId: orderAction.createdBy.id}) }}">
                                                                        {{ orderAction.createdBy.forename }} {{ orderAction.createdBy.surname }}</a>
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>Zaktualizowano</td>
                                                                <td>
                                                                    {{ orderAction.updatedAt|date }} przez
                                                                    <a href="{{ path('workflow_workman_show', {userId: orderAction.updatedBy.id}) }}">
                                                                        {{ orderAction.updatedBy.forename }} {{ orderAction.updatedBy.surname }}</a>
                                                                </td>
                                                            </tr>
                                                            <tr class="footer">
                                                                <td colspan="2">
                                                                    <a class="btn-edit" target="_blank">edytuj
                                                                        <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                                                    </a>
                                                                    <button class="btn-remove">usuń
                                                                        <i class="fa fa-trash-o" aria-hidden="true"></i>
                                                                    </button>
                                                                </td>
                                                            </tr>
                                                        </table>
                                                    </div>
                                                </td>
                                            </tr>
                                        {% endfor %}
                                    </table>
                                </div>
                            </td>
                        </tr>
                    {% endfor %}
                {% endif %}
                {% if orderHeader.orderServices.count > 0 %}
                    <tr class="service-header">
                        <th>L.p.</th>
                        <th colspan="3">Usługa</th>
                        <th>Cena jedn. netto</th>
                        <th>Ilość</th>
                        <th>J.m.</th>
                        <th>Netto</th>
                        <th>VAT</th>
                        <th>Brutto</th>
                    </tr>
                    {% for key, orderService in orderHeader.orderServices %}
                        <tr class="header">
                            <td class="td-center num">{{ key+1 }}.</td>
                            <td class="td-left" colspan="3">{{ orderService.service.name }}</td>
                            <td class="td-center">{{ orderService.unitPriceNet|money }}</td>
                            <td class="td-center">{{ orderService.quantity|money }}</td>
                            <td class="td-center">
                                {% if orderService.service.measure is not null %}
                                    {{ orderService.service.measure.shortcut }}
                                {% endif %}
                            </td>
                            <td class="td-center">{{ orderService.totalNet|money }}</td>
                            <td class="td-center">{{ orderService.vat|money }}</td>
                            <td class="td-center">{{ orderService.totalDue|money }}</td>
                        </tr>
                        <tr class="content">
                            <td colspan="10">
                                <div class="content">
                                    <table class="content info-table">
                                        <tr>
                                            <td>Nazwa usługi</td>
                                            <td>
                                                <a href="{{ path('service_index') }}">{{ orderService.service.name }}</a>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Cena jednostkowa netto</td>
                                            <td>{{ orderService.unitPriceNet|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Ilość</td>
                                            <td>{{ orderService.quantity|money }}
                                                {% if orderService.service.measure is not null %}
                                                    {{ orderService.service.measure.shortcut }}
                                                {% endif %}
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Wartość netto przed rabatem</td>
                                            <td>{{ orderService.totalNetBeforeDiscount|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Rabat</td>
                                            <td>{{ orderService.discountPc }}%</td>
                                        </tr>
                                        <tr>
                                            <td>Kwota rabatu</td>
                                            <td>{{ orderService.discount|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Wartość netto</td>
                                            <td>{{ orderService.totalNet|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>VAT</td>
                                            <td>{{ orderService.vatPc }}%</td>
                                        </tr>
                                        <tr>
                                            <td>Kwota VAT</td>
                                            <td>{{ orderService.vat|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Należność całkowita</td>
                                            <td>{{ orderService.totalDue|money }} zł</td>
                                        </tr>
                                        <tr>
                                            <td>Uwagi</td>
                                            <td>{{ orderService.remarks }}</td>
                                        </tr>
                                        <tr>
                                            <td>Wprowadzono</td>
                                            <td>
                                                {{ orderService.createdAt|date }} przez
                                                <a href="{{ path('workflow_workman_show', {userId: orderService.createdBy.id}) }}">
                                                    {{ orderService.createdBy.forename }} {{ orderService.createdBy.surname }}</a>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Zaktualizowano</td>
                                            <td>
                                                {{ orderService.updatedAt|date }} przez
                                                <a href="{{ path('workflow_workman_show', {userId: orderService.updatedBy.id}) }}">
                                                    {{ orderService.updatedBy.forename }} {{ orderService.updatedBy.surname }}</a>
                                            </td>
                                        </tr>
                                        <tr class="footer">
                                            <td colspan="2">
                                                <a class="btn-edit" target="_blank">edytuj
                                                    <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                                </a>
                                                <button class="btn-remove">usuń
                                                    <i class="fa fa-trash-o" aria-hidden="true"></i>
                                                </button>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </td>
                        </tr>
                    {% endfor %}
                {% endif %}
                <tr class="header summary">
                    <td class="td-right" colspan="7">Razem [zł]:</td>
                    <td class="td-center">{{ orderHeader.totalNet|money }}</td>
                    <td class="td-center">{{ orderHeader.vat|money }}</td>
                    <td class="td-center">{{ orderHeader.totalDue|money }}</td>
                </tr>
                <tr class="content">
                    <td colspan="10">
                        <div class="content">
                            <table class="content info-table">
                                <tr>
                                    <td>Wartość całkowita netto przed rabatem</td>
                                    <td>{{ orderHeader.totalNetBeforeDiscount|money }} zł</td>
                                </tr>
                                <tr>
                                    <td>Kwota rabatu</td>
                                    <td>{{ orderHeader.discount|money }} zł</td>
                                </tr>
                                <tr>
                                    <td>Wartość netto</td>
