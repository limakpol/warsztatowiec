<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Warsztatowiec{% endblock %}</title>
    {% block stylesheets %}
        {% stylesheets
        'assets/css/application/layout/*'
        filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        <link href="https://fonts.googleapis.com/css?family=Ubuntu&amp;subset=latin-ext" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Open+Sans|Ubuntu&amp;subset=latin-ext" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Fira+Sans&amp;subset=latin-ext" rel="stylesheet">
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
{% block body %}
<main>
    {% block main %}
    {% endblock %}
</main>
{% endblock %}
{% block javascripts %}

    {% javascripts
    'assets/js/jquery-3.2.1.min.js'
    'assets/js/font_awesome.js'
    'assets/js/application/layout.js'
    'assets/js/application/header/*'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <!--<script src="https://use.fontawesome.com/7bc874a398.js"></script> -->
    <!-- <script
            src="https://code.jquery.com/jquery-3.2.1.min.js"
            integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
            crossorigin="anonymous">
    </script> -->
{% endblock %}
</body>
</html>
