<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Warsztatowiec{% endblock %}</title>
    {% block stylesheets %}
        {% stylesheets
            'assets/css/homepage/layout.css'
        filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        <link href="https://fonts.googleapis.com/css?family=Ubuntu&amp;subset=latin-ext" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Open+Sans|Ubuntu&amp;subset=latin-ext" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Fira+Sans&amp;subset=latin-ext" rel="stylesheet">
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
{% block body %}
        {% block header %}
            <header>
                <img src="{{ asset('assets/img/logo.png') }}">
                <div id="menu-square">
                    <i class="fa fa-bars" aria-hidden="true"></i>
                </div>
                <ul>
                    <li class="disabled">funkcje</li>
                    <li class="disabled">cennik</li>
                    <li class="disabled">polecajÄ…</li>
                    <li><a href="{{ path('app_login') }}">logowanie</a></li>
                    <li><a href="{{ path('app_register') }}">rejestracja</a></li>
                </ul>
            </header>
        {% endblock %}
{% endblock %}
{% block javascripts %}
    <script src="https://use.fontawesome.com/7bc874a398.js"></script>
    <!-- <script
            src="https://code.jquery.com/jquery-3.2.1.min.js"
            integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
            crossorigin="anonymous">
    </script> -->
    <script src="{{ asset('assets/js/jquery-3.2.1.min.js') }}"></script>
{% endblock %}
</body>
</html>
